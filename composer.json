{
    "name": "suver/laravel-gelf-logger",
    "description": "A package to send logs to a gelf compatible backend like graylog",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "suver",
        "laravel-gelf",
        "gelf",
        "graylog",
        "laravel",
        "php"
    ],
    "authors": [
        {
            "name": "suver",
            "email": "suver@inbox.ru",
            "homepage": "https://farpse.com"
        }
    ],
    "support": {
        "issues": "https://github.com/suver/laravel-gelf-logger/issues",
        "source": "https://github.com/suver/laravel-gelf-logger",
        "email": "suver@inbox.ru"
    },
    "require": {
        "php": ">=5.6",
        "illuminate/support": "~5.2.0|~5.3.0|~5.4.0",
        "graylog2/gelf-php": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "orchestra/testbench": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "suver\\LaravelGelfLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "suver\\LaravelGelfLogger\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
